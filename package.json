{
  "name": "react-redux-starter-kit",
  "version": "0.1.0",
  "private": true,
  "description": "Starter kit for React+Redux+React Router projects",
  "keywords": [
    "frontend",
    "UI",
    "BEM",
    "typescript",
    "Create-react-app",
    "Redux",
    "React-Router",
    "Material UI",
    "starter kit"
  ],
  "homepage": "https://nzminusdev.github.io/react-redux-starter-kit/",
  "author": "Nikita Zaitsev <developer.n.zaytsev@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/jest": "^24.9.1",
    "@types/node": "^12.20.23",
    "@types/react": "^16.14.15",
    "@types/react-dom": "^16.9.14",
    "@types/react-redux": "^7.1.18",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-typescript": "^14.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-fsd": "^1.0.1",
    "eslint-plugin-no-loops": "^0.3.0",
    "eslint-plugin-no-use-extend-native": "^0.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-sonarjs": "^0.10.0",
    "gh-pages": "^3.2.3",
    "husky": "^7.0.2",
    "lint-staged": "^11.1.2",
    "prettier": "^2.3.2",
    "source-map-explorer": "^2.5.2"
  },
  "dependencies": {
    "@craco/craco": "^6.2.0",
    "@material-ui/core": "^5.0.0-beta.5",
    "@material-ui/icons": "^4.11.2",
    "@reduxjs/toolkit": "^1.6.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.5",
    "react-router-dom": "^5.3.0",
    "react-scripts": "4.0.3",
    "typescript": "^4.1.6"
  },
  "eslintConfig": {
    "extends": "./.eslintrc.js"
  },
  "prettier": {
    "endOfLine": "auto",
    "printWidth": 100,
    "singleQuote": true
  },
  "lint-staged": {
    "*": [
      "prettier --ignore-unknown --write"
    ],
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "craco test --watchAll=false --bail --findRelatedTests"
    ]
  },
  "scripts": {
    "start": "craco start",
    "test": "craco test",
    "analyze": "npm run build && source-map-explorer 'build/static/js/*.js'",
    "deploy": "gh-pages -d build",
    "predeploy": "npm run build",
    "build": "craco build",
    "prepare": "husky install"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
